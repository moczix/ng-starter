{
  "name": "ng-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "eslint-fix": "eslint --ext .ts,.html packages/ apps/ --fix",
    "eslint-check": "eslint --ext .ts,.html packages/ apps/",
    "tsc-check": "tsc --build tsconfig.tsc-check.json",
    "prettier-fix": "prettier --config .prettierrc --write '**/*.html'",
    "prettier-check": "prettier --config .prettierrc --check '**/*.html'",
    "test-all": "node --trace-warnings node_modules/.bin/jest --config=jest.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "~12.2.10",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jest": "27.0.0",
    "@types/node": "~16.6.0",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "jest": "27.0.6",
    "jest-preset-angular": "9.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "ts-node": "~10.2.0",
    "ts-jest": "27.0.4",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.3.2",
    "typescript": "~4.3.5"
  }
}
